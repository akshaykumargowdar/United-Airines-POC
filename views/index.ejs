		<div class="popup-box" id="animate">	
				<div class="popup-head" style =" background:#4c86af;">
					<div class="popup-head-left pull-left" style="color:#fff;"><img src="../images/chatbot.jpg" alt="IamChatBot" > <b> Agent Assistant</b></div>
						  <div class="popup-head-right pull-right">
							<button data-widget="remove" id="removeClass1" class="chat-header-button pull-right" type="button"><i class="glyphicon glyphicon-off"></i></button>
						  </div>
					</div>
				  
				<div class="popup-messages myscroll" scroll-glue>
				
					<!-- Message. Default to the left --> 
						<div>
							<div class="direct-chat-info clearfix">
								<span class="direct-chat-name pull-left">Agent Assistant</span>
							</div>
						 
	
						  <div  ng-repeat = "x in myData">
							<div class="direct-chat-text" ng-if = "x.text" ng-repeat="y in x.text">
							<p> {{ y }}</p>
							</div>
							
							 <div class ="direct-chat-question" ng-if = "x.question">
								<p> {{ x.question }}</p> 
							</div>
							
						  </div>
						  <div class="direct-chat-info clearfix">
							<span class="direct-chat-timestamp pull-right"> {{CurrentDate | date:'hh:mm:ss a'}}</span>
						  </div>
						</div>
				
				</div>
			
				<div class="popup-messages-footer" >
					<input class = "chatbotinput" style = "color :#000;" type="text" ng-keypress="($event.which === 13)?send():0" ng-model="input.question" / > 
					<button class="button1" ng-click="send()" > <i class="fa fa-paper-plane" aria-hidden="true"></i> </button> 
				</div>
		</div>
